#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/henry.lucco/dev/HighspotTypeAgent/ts/node_modules/.pnpm/@vscode+vsce@3.2.1/node_modules/@vscode/vsce/node_modules:/Users/henry.lucco/dev/HighspotTypeAgent/ts/node_modules/.pnpm/@vscode+vsce@3.2.1/node_modules/@vscode/node_modules:/Users/henry.lucco/dev/HighspotTypeAgent/ts/node_modules/.pnpm/@vscode+vsce@3.2.1/node_modules:/Users/henry.lucco/dev/HighspotTypeAgent/ts/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/henry.lucco/dev/HighspotTypeAgent/ts/node_modules/.pnpm/@vscode+vsce@3.2.1/node_modules/@vscode/vsce/node_modules:/Users/henry.lucco/dev/HighspotTypeAgent/ts/node_modules/.pnpm/@vscode+vsce@3.2.1/node_modules/@vscode/node_modules:/Users/henry.lucco/dev/HighspotTypeAgent/ts/node_modules/.pnpm/@vscode+vsce@3.2.1/node_modules:/Users/henry.lucco/dev/HighspotTypeAgent/ts/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@vscode/vsce/vsce" "$@"
else
  exec node  "$basedir/../@vscode/vsce/vsce" "$@"
fi
